                                                    <td>
                                                        <a href="{{=URL('eliminar', args=[edif.id,'edificio'])}}">
                                                            <img src="{{=URL('static','images/eliminar.png')}}" width="20px">
                                                        </a>
                                                    </td>
                                                </tr>
                                                {{pass}}
                                            {{else:}}
