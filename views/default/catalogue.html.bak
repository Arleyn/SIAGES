<script  type="text/javascript">
$( document ).ready(function() {
    cod = $("#cod").val();
    if (cod == 'tabla_area'){
        document.getElementById('tabla_unidades').style.display = "none";
        document.getElementById('tabla_area').style.display = "block";
        document.getElementById('tabla_edificios').style.display = "none";
        document.getElementById('tabla_inventario').style.display = "none";
        document.getElementById('tabla_empleado').style.display = "none";
        document.getElementById('tabla_supervisores').style.display = "none";
        document.getElementById('tabla_solicitudes').style.display = "none";
        document.getElementById('tabla_prioridad').style.display = "none";
        document.getElementById('tabla_lugar').style.display = "none";
        document.getElementById('tabla_unidades_metricas').style.display = "none";
    } else if (cod == 'tabla_unidadMetrica') {
        document.getElementById('tabla_unidades_metricas').style.display = "block";
        document.getElementById('tabla_unidades').style.display = "none";
        document.getElementById('tabla_area').style.display = "none";
        document.getElementById('tabla_edificios').style.display = "none";
        document.getElementById('tabla_inventario').style.display = "none";
