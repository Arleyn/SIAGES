<!DOCTYPE html>
{{extend 'layout.html'}}
{{block content}}
<html>
<input type="hidden" id="cod" value="{{=cod}}">
        <!-- Page Content -->
        <row>
          <ul class="nav navbar-nav navbar-right">
            {{='auth' in globals() and auth.navbar('Welcome',mode='dropdown') or ''}}
          </ul>
        </row>
        <row>
          <center>
            <div class="btn-group">
              <button type="button" class="btn btn-default" onclick="show('tabla_area')">Áreas de Trabajo</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_edificios')">Edificios</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_unidades')">Unidad</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_lugar')">Lugar</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_empleado')">Empleados</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_supervisores')">Supervisores</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_inventario')">Inventario</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_unidades_metricas')">Unidad Métricas</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_prioridad')">Prioridad</button>
              <button type="button" class="btn btn-default" onclick="show('tabla_solicitudes')">Solicitud-Estatus</button>
            </div>
          </center>
        </row>

            <div class="container-fluid" id="tabla_empleado">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="rounded">
                                <div class="roundedwhitecatalogo">
                                    <center>
                                        <h3>Catálogo de Empleados</h3>
                                        <hr WIDTH="30%">
                                        <p>Lista de los Empleados</p>
                                    </center>

                                    <table class="table table-hover table-striped ">
                                        <thead>
                                            <th class="hColumns">Nombre</th>
                                            <th class="hColumns">Apellido</th>
                                            <th class="hColumns">USBId</th>
                                            <th class="hColumns">Cargo</th>
                                            <th class="hColumns">Área</th>
                                            <th class="hColumns">Tlf</th>
                                        </thead>
                                        <tbody>
                                            {{if len(personal):}}
                                                {{for persona in personal:}}
                                                <tr class="clickable">
                                                    <td class="hColumns" >{{=(persona.nombre)}}</td>
                                                    <td class="hColumns">{{=(persona.apellido)}}</td>
                                                    <td class="hColumns">{{=(persona.identificador)}}</td>
                                                    <td class="hColumns">{{=(persona.cargo)}}</td>
                                                    <td class="hColumns">{{=(persona.area.nombre)}}</td>
                                                    <td class="hColumns">{{=(persona.tlf)}}</td>
                                                </tr>
                                                {{pass}}
                                            {{else:}}
                                                <center>
                                                    <td>No hay ningun personal registrado en el catálogo</td>
                                                </center>
                                            {{pass}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="container-fluid" id="tabla_lugar">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="rounded">
                                <div class="roundedwhitecatalogo">
                                    <button type="button" class="col-sm-offset-10 btn btn-success" data-toggle="modal" data-target="#agregarLugar">Agregar Lugar</button>
                                    <center>
                                        <h3>Catálogo de Lugares</h3>
                                        <hr WIDTH="30%">
                                        <p>Lista de los Lugares de la USB</p>
                                    </center>

                                    <table class="table table-hover table-striped ">
                                        <thead>
                                            <th class="hColumns">Edificio</th>
                                            <th class="hColumns">Espacio</th>
                                            <th class="hColumns">Referencia</th>
                                            <th class="hColumns">Extencion Tlf</th>
                                            <th></th>
                                        </thead>
                                        <tbody>
                                            {{if len(lugar):}}
                                                {{for lugar in lugar:}}
                                                <tr class="clickable">
                                                    <td class="hColumns" >{{=(lugar.edificio.nombre)}}</td>
                                                    <td class="hColumns">{{=(lugar.espacio)}}</td>
                                                    <td class="hColumns">{{=(lugar.referencia)}}</td>
                                                    <td class="hColumns">{{=(lugar.extension_tlf)}}</td>
                                                    <td>
                                                        <a href="{{=URL('mod_catalogo', args=[lugar.id,'lugar'])}}">
                                                            <img src="{{=URL('static','images/editar2.png')}}" width="20px">
                                                        </a>
                                                    </td>
                                                    <td>
                                                        <a href="{{=URL('eliminar', args=[lugar.id,'lugar'])}}">
                                                            <img src="{{=URL('static','images/eliminar.png')}}" width="20px">
                                                        </a>
                                                    </td>
                                                </tr>
                                                {{pass}}
                                            {{else:}}
                                                <center>
                                                    <td>No hay ningun lugar registrado en el catálogo</td>
                                                </center>
                                            {{pass}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                </div>

                 <div class="container-fluid" id="tabla_edificios">
